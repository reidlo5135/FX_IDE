<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.ide.fx_ide.root.RootController" prefWidth="485" prefHeight="380">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <children>
        <MenuBar fx:id="mb_top" id="mb_top" prefWidth="1580" prefHeight="30" />
        <TextArea fx:id="ta_code" id="ta_code" layoutX="480" layoutY="30" prefWidth="1200" prefHeight="550" />
        <Button fx:id="btn_compile" onAction="#onCompileButtonClick" id="btn_compile" layoutX="1620" layoutY="580">
            <graphic>
                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@static/image/play-solid.svg"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>
