<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.ide.fx_ide.controller.editor.CreatorSceneController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <children>
        <Label fx:id="label_path" id="label_path" layoutX="55" layoutY="20" />
        <TextField fx:id="tf_name" id="tf_name" layoutX="55" layoutY="50" prefWidth="200" />
        <Button fx:id="btn_create" onAction="#onCreateButtonClick" id="btn_create" layoutX="55" layoutY="80" text="Create" prefWidth="200" />
    </children>
</AnchorPane>
